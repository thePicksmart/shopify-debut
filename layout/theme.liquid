  {% if settings.favicon != blank %}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
  {% endif %}

  {% capture seo_title %}
    {{ page_title }}
    {% if current_tags %}
      {%- assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags -}}
    {% endif %}
    {% if current_page != 1 %}
      &ndash; {{ 'general.meta.page' | t: page: current_page }}
    {% endif %}
    {% unless page_title contains shop.name %}
      &ndash; {{ shop.name }}
    {% endunless %}
  {% endcapture %}
  <title>{{ seo_title }}</title>

  {% if page_description %}
    <meta name="description" content="{{ page_description | escape }}">
  {% endif %}
  
  {% if seo_title contains 'Premium home decor | Premium kitchenware | Trendy clothing' %}
    <meta name="keywords" content="premium home decor, premium kitchenware, western clothing collections, trendy clothing and accessories, selected bedroom collections, premium kitchen collections, premium electronic appliances, premium home appliances online, home decor products, kitchen products online, trendy clothing and accessories" >
  {% elsif seo_title contains 'Selected bedroom collections | Selected bedroom accessories' %}
    <meta name="keywords" content="selected bedroom collections, selected bedroom accessories, buy selected bedroom collections, bedroom collections for sale, bedroom accessories for sale">
  {% elsif seo_title contains 'Premium kitchen collections | Premium kitchen appliances' %}
    <meta name="keywords" content="premium kitchen collections, premium kitchen appliances, premium kitchenware, premium kitchenware online, kitchenware products, premium kitchenware products">
  {% elsif seo_title contains 'Western clothing collections | Western clothing accessories online' %}
    <meta name="keywords" content="western clothing collections, western clothing accessories online, western wear clothing collections, western wear clothing accessories online, clothing collection, clothing collection online, clothing accessories online">
  {% elsif seo_title contains 'Premium electronic appliances | Premium home appliances online' %}
    <meta name="keywords" content="premium electronic appliances, premium home appliances online, western electronics home appliances, home appliances online, home electronics appliances">
  {% elsif seo_title contains 'Home kitchen tips | Home decor tips | Accessories tips' %}
    <meta name="keywords" content="home kitchen tips, home decor tips , accessories tips, easy tips for home decor, easy tips for accessories">
  {% elsif seo_title contains 'Factory direct home goods | Factory direct home products' %}
    <meta name="keywords" content="factory direct home goods, factory direct home products, thepicksmart, factory home products, factory goods, selected factory goods, reasonable price factory goods, premium quality factory goods">
  {% endif %}

  {% include 'social-meta-tags' %}

  {{ 'theme.scss.css' | asset_url | stylesheet_tag }}
  {% include 'google-fonts' %}

  <script>
    var theme = {
      strings: {
        addToCart: {{ 'products.product.add_to_cart' | t | json }},
        soldOut: {{ 'products.product.sold_out' | t | json }},
        unavailable: {{ 'products.product.unavailable' | t | json }},
        showMore: {{ 'general.filters.show_more' | t | json }},
        showLess: {{ 'general.filters.show_less' | t | json }},
        addressError: {{ 'sections.map.address_error' | t | json }},
        addressNoResults: {{ 'sections.map.address_no_results' | t | json }},
        addressQueryLimit: {{ 'sections.map.address_query_limit_html' | t | json }},
        authError: {{ 'sections.map.auth_error_html' | t | json }}
      },
      moneyFormat: {{ shop.money_format | json }}
    }

    document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
  </script>

  <!--[if (lte IE 9) ]>{{ 'match-media.min.js' | asset_url | script_tag }}<![endif]-->

  {% if template.directory == 'customers' %}
    <!--[if (gt IE 9)|!(IE)]><!--><script src="{{ 'shopify_common.js' | shopify_asset_url }}" defer="defer"></script><!--<![endif]-->
    <!--[if lte IE 9]><script src="{{ 'shopify_common.js' | shopify_asset_url }}"></script><![endif]-->
  {% endif %}

  <!--[if (gt IE 9)|!(IE)]><!--><script src="{{ 'lazysizes.js' | asset_url }}" async="async"></script><!--<![endif]-->
  <!--[if lte IE 9]><script src="{{ 'lazysizes.min.js' | asset_url }}"></script><![endif]-->

  <!--[if (gt IE 9)|!(IE)]><!--><script src="{{ 'vendor.js' | asset_url }}" defer="defer"></script><!--<![endif]-->
  <!--[if lte IE 9]><script src="{{ 'vendor.js' | asset_url }}"></script><![endif]-->

  <!--[if (gt IE 9)|!(IE)]><!--><script src="{{ 'theme.js' | asset_url }}" defer="defer"></script><!--<![endif]-->
  <!--[if lte IE 9]><script src="{{ 'theme.js' | asset_url }}"></script><![endif]-->
  {{ content_for_header }}
  
